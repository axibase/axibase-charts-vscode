{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "ATSD Visualization",
	"patterns": [
		{
			"include": "#keywords"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.other.unit.atsd-visual",
					"match": "\\[(configuration|widget|series|tags|group|node|link)\\]"
				},
				{
					"name": "keyword.control.atsd-visual",
					"match": "\\b(for|endfor|in|list)\\b"
				},
				{
					"name": "comment.block.atsd-visual",
					"match": "/\\*.*\\*/"
				},
				{
					"name": "comment.line.atsd-visual",
					"match": "#.*$"
				},
				{
					"name": "variable.name.atsd-visual",
					"match": "\\b[-a-zA-Z_]+[-a-zA-Z_0-9]+\\b"
				}
			]
		}
	},
	"scopeName": "source.av"
}